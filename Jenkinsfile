pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build jenkinsfile'
                }
            }
        }
        stage('test') {
            steps {
                echo 'Test the jenkinsfile'
            }
        }
        stage ('deploy') {
            steps {
                echo 'deploy the jenkinsfile'
            }
        }   
}
